{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/nft_details/(.*)": "<rootDir>/../libs/nft_details/src/$1",
    "@app/nft_details": "<rootDir>/../libs/nft_details/src"
  }
}